###  https://gallosiciliani.unict.it/ns/gs-features#Afer
:Afer rdf:type owl:Class ;
      rdfs:subClassOf liph:LinguisticPhenomenon ;
      owl:disjointWith :Assib ,
                       :Degem ,
                       :Deretr ,
                       :Dissim ,
                       :Ditt ,
                       :Elim ,
                       :Leniz ,
                       :Palat ,
                       :Vocal ;
      rdfs:comment "The loss or deletion of one or more sounds at the beginning of a word." ;
      rdfs:label "Apheresis" .


###  https://gallosiciliani.unict.it/ns/gs-features#Assib
:Assib rdf:type owl:Class ;
       rdfs:subClassOf liph:LinguisticPhenomenon ;
       owl:disjointWith :Degem ,
                        :Deretr ,
                        :Dissim ,
                        :Ditt ,
                        :Elim ,
                        :Leniz ,
                        :Palat ,
                        :Vocal ;
       rdfs:comment "The process by which non-sibilant consonants (typi- cally plosives or affricates) change into sibilant sounds." ;
       rdfs:label "Assibilation" .


###  https://gallosiciliani.unict.it/ns/gs-features#Degem
:Degem rdf:type owl:Class ;
       rdfs:subClassOf liph:LinguisticPhenomenon ;
       owl:disjointWith :Deretr ,
                        :Dissim ,
                        :Ditt ,
                        :Elim ,
                        :Leniz ,
                        :Palat ,
                        :Vocal ;
       rdfs:comment "The reduction of all long (or double) consonants into short (or single) ones." ;
       rdfs:label "Degemination" .


###  https://gallosiciliani.unict.it/ns/gs-features#Deretr
:Deretr rdf:type owl:Class ;
        rdfs:subClassOf liph:LinguisticPhenomenon ;
        owl:disjointWith :Dissim ,
                         :Ditt ,
                         :Elim ,
                         :Leniz ,
                         :Palat ,
                         :Vocal ;
        rdfs:comment "The elimination of the retroflexed pronunciation of a consonant." ;
        rdfs:label "Deretroflexion" .


###  https://gallosiciliani.unict.it/ns/gs-features#Dissim
:Dissim rdf:type owl:Class ;
        rdfs:subClassOf liph:LinguisticPhenomenon ;
        owl:disjointWith :Ditt ,
                         :Elim ,
                         :Leniz ,
                         :Palat ,
                         :Vocal ;
        rdfs:comment "The process by which two adjacent segments that share some features change to become less like each other." ;
        rdfs:label "Dissimilation"@en .


###  https://gallosiciliani.unict.it/ns/gs-features#Ditt
:Ditt rdf:type owl:Class ;
      rdfs:subClassOf liph:LinguisticPhenomenon ;
      owl:disjointWith :Elim ,
                       :Leniz ,
                       :Palat ,
                       :Vocal ;
      rdfs:comment "The turning of a stressed vowel into a diphthong, which is a (functionally) single vowel that starts out in the position of one monophthong and ends up in the position of another." ;
      rdfs:label "Diphthongization"@en .


###  https://gallosiciliani.unict.it/ns/gs-features#Elim
:Elim rdf:type owl:Class ;
      rdfs:subClassOf liph:LinguisticPhenomenon ;
      owl:disjointWith :Leniz ,
                       :Palat ,
                       :Vocal ;
      rdfs:comment "Elimination of some allophonic variants according to a specific phonological context, such as the unstressed position." ;
      rdfs:label "Elimination" .


###  https://gallosiciliani.unict.it/ns/gs-features#GalloSicilianFeature
:GalloSicilianFeature rdf:type owl:Class ;
                      rdfs:subClassOf liph:LinguisticPhenomenon ;
                      rdfs:comment "This class represents linguistic phenomena occurred in borrowing of Sicilian expression in Gallo-Sicilian varieties." ;
                      rdfs:label "Gallo-Sicilian feature" .


###  https://gallosiciliani.unict.it/ns/gs-features#Inf
:Inf rdf:type owl:Class ;
     rdfs:subClassOf :Palat .


###  https://gallosiciliani.unict.it/ns/gs-features#Leniz
:Leniz rdf:type owl:Class ;
       rdfs:subClassOf liph:LinguisticPhenomenon ;
       owl:disjointWith :Palat ,
                        :Vocal ;
       rdfs:comment "The softening of the intervocalic voiceless (both fricative and plosive) consonants." ;
       rdfs:label "Lenition" .


###  https://gallosiciliani.unict.it/ns/gs-features#Palat
:Palat rdf:type owl:Class ;
       rdfs:subClassOf liph:LinguisticPhenomenon ;
       owl:disjointWith :Vocal ;
       rdfs:comment "Vocalic palatalization, i.e., the phonological process by which the low, central vowel /a/ shifts towards a more fronted and sometimes slightly raised position, typically resulting in a vowel like /æ/, /E/, or /e/, in a palatal or fronting phonetic environment." ;
       rdfs:label "Palatalization" .


###  https://gallosiciliani.unict.it/ns/gs-features#Vocal
:Vocal rdf:type owl:Class ;
       rdfs:subClassOf liph:LinguisticPhenomenon ;
       rdfs:comment """Representing the adaptation of a loanword to the vocalic system of the receiving
language.""" ;
       rdfs:label "Vocalization" .


